{
  "name": "underscores",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called _s, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "browser-sync": "^2.29.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.26",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.13"
  },
  "scripts": {
    "build:css": "postcss css/main.css -o css/main-min.css",
    "watch:css": "chokidar 'css/**/*.css' -c 'npm run build:css'",
    "serve": "browser-sync start --proxy 'limitless.local' --files '**/*.php' 'css/*.css' 'js/*.js' --no-notify",
    "dev": "concurrently 'npm run watch:css' 'npm run serve'"
  }
}
